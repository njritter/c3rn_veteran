rbind(Q38_general, Q38_bask, Q38_botanist, Q38_healing_rose) %>%
tidy_choose_all()
number_of_people <- length(unique(Q38_all$id))
# Manipulate data into a graphable format
Q38_plot <-
Q38_all %>%
group_by(chosen) %>%
summarize(count = n()) %>%
mutate(percent_all = count / sum(count)) %>%
mutate(percent_responded = count / number_of_people)
# Plot with percents and response counts
r <-
