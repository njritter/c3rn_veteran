q_final_response <- "X.311"
q_data <- get_choose_all(raw_data, q.name = q, f.response = q_final_response)
table_1 <- get_single_selection_2(q_data, "Tinctures") %>%
filter(response != "N/A") %>%
clean_single_select_2()
table_1_n <- tally(table_1, count)
r1 <- plot_single_select(table_1) +
labs(title = "Tinctures", subtitle = table_1_n)
r1
# Load libraries
library(tidyverse)
library(ggplot2)
library(stringr)
source("helper.R")
raw_data <- read.csv("data/master.csv", stringsAsFactors = FALSE)
q <- 'How.many.milliliters..mL..of.tincture.are.you.using.per.week..Note..30mL...1oz'
q_final_response <- "X.311"
q_data <- get_choose_all(raw_data, q.name = q, f.response = q_final_response)
table_1 <- get_single_selection_2(q_data, "Tinctures") %>%
filter(response != "N/A") %>%
clean_single_select_2()
table_1_n <- tally(table_1, count)
r1 <- plot_single_select(table_1) +
labs(title = "Tinctures", subtitle = table_1_n)
r1
# Save graph as PNG, plot data as csv, and all question data including response IDs as csv (save question n and N at end of plot file as "_x.x")
ggsave(filename = 'q65_Tinctures_wkly.png', plot = r1, device = 'png')
write.csv(table_1, 'q65_Tinctures_wkly_71.csv', row.names = FALSE)
