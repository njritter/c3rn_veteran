library(tidyverse)
library(ggplot2)
library(stringr)
source("helper.R")
raw_data <- read.csv("data/njsurvey.csv", stringsAsFactors = FALSE)
q15 <- "New.Jersey.should.implement.a.loan.or.grant.fund.to.support.those.negatively.impacted.by.the.drug.war.for.non.felony.related.cannabis.convictions.to.have.access.to.capital.to.enter.the.industry."
q15_t <- "New Jersey should implement a loan or grant fund to support those negatively impacted by the drug war for non felony related cannabis convictions to have access to capital to enter the industry."
q15_data <- get_single_selection(data = raw_data, q.name = q15)
q15_n <- length(unique(q15_data$id))
q15_plot <- clean_single_select(q15_data)
r15 <- plot_single_select(q15_plot, q15_t, q15_n)
r15
row3 <- colnames(q15_plot)
q15_plot2 <- rbind(row3, q15_plot)
names(q15_plot2) <- c("q15", q15_t, paste("n = ", q15_n))
dir.create("output/q15_grants_for_impacted")
ggsave(filename = 'output/q15_grants_for_impacted/q15_grants_for_impacted.png', plot = r15, device = 'png')
write.csv(q15_plot2, paste0("output/q15_grants_for_impacted/q15_grants_for_impacted_", q15_n,".csv"), row.names=F)
